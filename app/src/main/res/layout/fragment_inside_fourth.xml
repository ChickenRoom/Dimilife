            android:layout_height="wrap_content"
            android:layout_marginTop="103dp"
            android:layout_gravity="center_horizontal">
            <ImageView
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:background="@drawable/gym_clothes"
